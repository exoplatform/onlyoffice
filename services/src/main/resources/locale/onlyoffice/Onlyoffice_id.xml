<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <OnlyofficeOpen>Edit di OnlyOffice</OnlyofficeOpen>
    </tooltip>
  </UIActionBar>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <onlyofficeOpen>Edit di OnlyOffice</onlyofficeOpen>
    </label>
  </UITabForm>
  
  <!-- For new document form -->
  <UINewDocumentForm>    <label>
      <option>
        <MicrosoftOfficeDocument>Microsoft Word Document (*.docx)</MicrosoftOfficeDocument>
        <MicrosoftOfficeSpreadsheet>Microsoft Excel Workbook (*.xlsx)</MicrosoftOfficeSpreadsheet>
        <MicrosoftOfficePresentation>Microsoft PowerPoint Presentation (*.pptx)</MicrosoftOfficePresentation>
        <MicrosoftOfficeNewSheet>Lembar</MicrosoftOfficeNewSheet>
      </option>
    </label>
  </UINewDocumentForm>  
  
  <!-- Messages -->
  <OnlyofficeEditor>
    <message>
      <ViewerNotFound>OnlyOffice editor tidak ditemukan</ViewerNotFound>
    </message>
    <error>
      <EditorCannotBeCreated>Editor tidak dapat dibuat. Silakan coba lagi.</EditorCannotBeCreated>
      <CannotReadDocument>Tidak dapat membaca dokumen. Silakan coba lagi.</CannotReadDocument>
      <CannotCreateEditor>Tidak dapat membuat editor. Silakan coba lagi.</CannotCreateEditor>
      <CannotSendEditorConfiguration>Tidak dapat mengirim konfigurasi editor. Silakan coba lagi.</CannotSendEditorConfiguration>
      <CannotInitEditorConfiguration>Tidak dapat menginisialisasi konfigurasi editor. Silakan coba lagi.</CannotInitEditorConfiguration>
      <InitializationError>Kesalahan saat menginisialisasi editor. Silakan coba lagi.</InitializationError>
      <DocumentIdRequired>Permintaan salah: ID dokumen diperlukan.</DocumentIdRequired>
    </error>
  </OnlyofficeEditor>
</bundle>
