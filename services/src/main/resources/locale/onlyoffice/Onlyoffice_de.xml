<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <OnlyofficeOpen>In OnlyOffice bearbeiten</OnlyofficeOpen>
    </tooltip>
  </UIActionBar>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <onlyofficeOpen>In OnlyOffice bearbeiten</onlyofficeOpen>
    </label>
  </UITabForm>
  
  <!-- For new document form -->
  <UINewDocumentForm>    <label>
      <option>
        <MicrosoftOfficeDocument>Microsoft Word Dokument (*.docx)</MicrosoftOfficeDocument>
        <MicrosoftOfficeSpreadsheet>Microsoft Excel Workbook (*.xlsx)</MicrosoftOfficeSpreadsheet>
        <MicrosoftOfficePresentation>Microsoft PowerPoint Pr√§sentation (*.pptx)</MicrosoftOfficePresentation>
        <MicrosoftOfficeNewSheet>Sheet</MicrosoftOfficeNewSheet>
      </option>
    </label>
  </UINewDocumentForm>  
  
  <!-- Messages -->
  <OnlyofficeEditor>
    <message>
      <ViewerNotFound>OnlyOffice Dokument-Editor nicht gefunden</ViewerNotFound>
    </message>
    <error>
      <EditorCannotBeCreated>Der Editor kann nicht erstellt werden. Bitte erneut versuchen.</EditorCannotBeCreated>
      <CannotReadDocument>Unlesbares Dokument. Bitte erneut versuchen.</CannotReadDocument>
      <CannotCreateEditor>Editor kann nicht erstellt werden. Bitte erneut versuchen.</CannotCreateEditor>
      <CannotSendEditorConfiguration>Editorkonfiguration kann nicht gesendet werden. Bitte erneut versuchen.</CannotSendEditorConfiguration>
      <CannotInitEditorConfiguration>Editorkonfiguration kann nicht initialisiert werden. Bitte erneut versuchen.</CannotInitEditorConfiguration>
      <InitializationError>Fehler beim Initialisieren des Editors. Bitte erneut versuchen.</InitializationError>
      <DocumentIdRequired>Falsche Anfrage: Dokument-ID erforderlich.</DocumentIdRequired>
    </error>
  </OnlyofficeEditor>
</bundle>
